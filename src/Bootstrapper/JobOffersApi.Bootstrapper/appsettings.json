{
  "app": {
    "name": "JobOffersApi"
  },
  "auth": {
    "issuerSigningKey": "",
    "issuer": "",
    "validIssuer": "",
    "validateAudience": false,
    "validateIssuer": true,
    "validateLifetime": true,
    "expiry": "07.00:00:00",
    "cookie": {
      "httpOnly": true,
      "sameSite": "unspecified",
      "secure": false
    }
  },
  "cors": {
    "allowCredentials": true,
    "allowedOrigins": [
      ""
    ],
    "allowedMethods": [
      "POST",
      "PUT",
      "DELETE"
    ],
    "allowedHeaders": [
      "Content-Type",
      "Authorization"
    ],
    "exposedHeaders": [
      "Resource-ID"
    ]
  },
  "logger": {
    "level": "information",
    "overrides": {
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
      "Microsoft.EntityFrameworkCore.Infrastructure": "Warning"
    },
    "excludePaths": [
      "/",
      "/ping",
      "/metrics"
    ],
    "excludeProperties": [
      "api_key",
      "access_key",
      "ApiKey",
      "ApiSecret",
      "ClientId",
      "ClientSecret",
      "ConnectionString",
      "Password",
      "Email",
      "Login",
      "Secret",
      "Token"
    ],
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "seq": {
      "enabled": true,
      "url": "",
      "apiKey": ""
    },
    "tags": {}
  },
  "messaging": {
    "useAsyncDispatcher": false
  },
  "outbox": {
    "enabled": false,
    "interval": "00:00:01"
  },
    "msSqlServer": {
        "connectionString": "Server=(localdb)\\mssqllocaldb;Database=JobOffersApi;Trusted_Connection=True;"
    },
  "security": {
    "encryption": {
      "enabled": true,
      "key": ""
    }
  }
}